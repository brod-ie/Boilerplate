{
  "name": "Brodie-Boilerplate",
  "private": true,
  "main": "./app/app.js",
  "scripts": {
    "init": "npm install gulp -g && npm install coffee-script -g && npm install browserify-shim -g && npm install && echo '{\"env\":\"develop\"}' > config.json",
    "test": "npm run init && npm run build && gulp test",
    "build": "npm install && gulp build --require coffee-script/register",
    "start": "supervisor -e 'js|css|jade|png|jpeg|json' app/app.js"
  },
  "author": "Ryan Brodie <ryanbrodie@me.com>",
  "license": "MIT",
  "dependencies": {
    "animate.css": "^3.1.1",
    "backbone": "^1.1.2",
    "body-parser": "^1.14.1",
    "bootstrap-browserify": "^2.1.1",
    "bootstrap-sass": "^3.2.0",
    "bower": "^1.3.8",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "coffee-script": "^1.8.0",
    "coffeeify": "^2.0.1",
    "coffeelint-stylish": "^0.1.0",
    "compression": "^1.0.9",
    "crypto": "0.0.3",
    "express": "^4.6.1",
    "frisby": "git+https://github.com/vlucas/frisby.git",
    "gulp": "^3.8.6",
    "gulp-autoprefixer": "0.0.8",
    "gulp-coffee": "^2.1.1",
    "gulp-coffeelint": "^0.3.3",
    "gulp-cssimport": "^1.2.3",
    "gulp-define-module": "^0.1.1",
    "gulp-if": "^1.2.4",
    "gulp-jade": "^0.6.1",
    "gulp-jasmine": "^2.1.0",
    "gulp-minify-css": "^0.3.7",
    "gulp-notify": "^1.4.0",
    "gulp-replace": "^0.4.0",
    "gulp-sass": "^2.1.1",
    "gulp-util": "^3.0.0",
    "http": "0.0.0",
    "jade": "^1.4.2",
    "jasmine-node": "^1.14.5",
    "jquery": "^2.1.1",
    "lodash": "^3.0.0",
    "longjohn": "^0.2.9",
    "minifyify": "^7.2.1",
    "neo4j": "^1.1.0",
    "node-jasmine-async": "^0.1.0",
    "normalize-css": "^2.3.1",
    "require": "^0.5.0",
    "socket.io": "^1.0.6",
    "socket.io-client": "^1.3.7",
    "supervisor": "^0.6.0",
    "tracer": "^0.8.0",
    "vinyl-source-stream": "^0.1.1"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jade": "jade/runtime"
  }
}
